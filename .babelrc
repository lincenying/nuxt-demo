{
    "presets": ["es2015", "stage-2"],
    "plugins": [
        "transform-export-extensions",
        ["transform-runtime", {
            "polyfill": true,
            "regenerator": true
        }],
        ["component", [{
            "libraryName": "element-ui",
            "styleLibraryName": "theme-default"
        }]]
    ],
    "comments": false
}
